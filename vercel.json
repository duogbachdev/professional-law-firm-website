{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "people-detail.html",
      "use": "@vercel/static"
    },
    {
      "src": "service-detail.html",
      "use": "@vercel/static"
    },
    {
      "src": "achievements.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/people-detail.html",
      "dest": "/people-detail.html"
    },
    {
      "src": "/service-detail.html",
      "dest": "/service-detail.html"
    },
    {
      "src": "/achievements.html",
      "dest": "/achievements.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
